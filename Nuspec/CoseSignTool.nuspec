<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>CoseSignTool</id>
    <version>$VersionNgt$</version>
    <authors>Microsoft</authors>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
Command line tool to sign files using COSE Sign1 message envelopes in a way that is compatible with Supply Chain Integrity Transparency and Trust (SCITT).
    </description>
    <dependencies>
		<group targetFramework=".NET8.0">
			<dependency id="CoseHandler" version="$VersionNgt$" />
		</group>
	</dependencies>
	<readme>ReadMe.md</readme>
	<license type="file">LICENSE</license>
  </metadata>
  <files>
	<file src="..\CoseSignTool\bin\Release\net8.0\CoseSignTool.exe" target="lib\net8.0" />
	<file src="..\CoseSignTool\bin\Release\net8.0\CoseSignTool.dll" target="lib\net8.0" />
	<file src="..\CoseSignTool\bin\Release\net8.0\CoseSignTool.pdb" target="Build\symbols" />
	<file src="..\CoseSignTool\bin\Release\net8.0\_manifest\spdx_2.2\*.*" target="Build\sbom\spdx_2.2" />
	<file src="..\docs\*.md" target="Docs" />
	<file src="..\*.md" target="." />
	<file src="..\LICENSE*" target="." />
  </files>
</package>
